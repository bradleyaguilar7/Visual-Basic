Private Sub cumputeTax_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cumputeTax.Click

        txtTaxDue.Text = 0.03 * (txtIncome.Text * numDependents.Value)

    End Sub

    Private Sub Reset_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Reset.Click

        txtIncome.Text = "0"
        NumDependents.Text = "0"
        txtTaxDue.Text = "0"

    End Sub
End Class
